<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="care_petrol_fleet_vehicle_odometer_view_form" model="ir.ui.view">
            <field name="name">care.petrol.fleet.vehicle.odometer.form</field>
            <field name="model">fleet.vehicle.odometer</field>
            <field name="inherit_id" ref="fleet.fleet_vehicle_odometer_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vehicle_id']" position="before">
                    <field name="use_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>